{
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "window.zoomLevel": 2,
  "files.autoSave": "onWindowChange",
  "files.autoSaveDelay": 100,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "terminal.external.osxExec": "iterm.app",
  "emmet.includeLanguages": {
    "html": "html",
    "erb": "erb",
    "javascrpit": "js",
    "vue": "html"
  },
  "typescript.validate.enable": false,
  "files.insertFinalNewline": true,
  "html.format.endWithNewline": true,
  "editor.detectIndentation": false,
  "editor.insertSpaces": false,
  "vetur.validation.template": false,
  "javascript.validate.enable": false,
  "editor.fontWeight": "500",
  "files.exclude": {
    "**/__pycache__": true
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "workbench.editor.showTabs": true,
  "csv-preview.theme": "material",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#2979FF",
    "activityBar.activeBorder": "#2979FF",
    "list.activeSelectionForeground": "#2979FF",
    "list.inactiveSelectionForeground": "#2979FF",
    "list.highlightForeground": "#2979FF",
    "scrollbarSlider.activeBackground": "#2979FF50",
    "editorSuggestWidget.highlightForeground": "#2979FF",
    "textLink.foreground": "#2979FF",
    "progressBar.background": "#2979FF",
    "pickerGroup.foreground": "#2979FF",
    "tab.activeBorder": "#2979FF",
    "notificationLink.foreground": "#2979FF",
    "editorWidget.resizeBorder": "#2979FF",
    "editorWidget.border": "#2979FF",
    "settings.modifiedItemIndicator": "#2979FF",
    "settings.headerForeground": "#2979FF",
    "panelTitle.activeBorder": "#2979FF",
    "breadcrumb.activeSelectionForeground": "#2979FF",
    "menu.selectionForeground": "#2979FF",
    "menubar.selectionForeground": "#2979FF",
    "editor.findMatchBorder": "#2979FF",
    "selection.background": "#2979FF40",
    "statusBarItem.remoteBackground": "#2979FF"
  },
  "material-icon-theme.activeIconPack": "react",
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.rendererType": "dom",
  "live2d.bottom": -50,
  "live2d.model": "miku",
  "telemetry.enableTelemetry": false,
  "live2d.height": 250,
  "live2d.right": -70,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "prettier.trailingComma": "es5",
  "prettier.useTabs": false,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.jsxBracketSameLine": true,
  "markdown-preview-enhanced.codeBlockTheme": "github.css",
  "markdown-preview-enhanced.previewTheme": "atom-dark.css",
  "sync.gist": "8a623dc83259ef71f0d67dad906cf88f",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.semi": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "python.languageServer": "Pylance",
  "materialTheme.accent": "Blue",
  "workbench.startupEditor": "welcomePage",
  "editor.suggest.showStatusBar": true,
  "workbench.preferredDarkColorTheme": "Material Theme High Contrast",
  "workbench.preferredHighContrastColorTheme": "Material Theme High Contrast",
  "workbench.preferredLightColorTheme": "Material Theme Lighter High Contrast",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown-pdf.headerTemplate": "<div style=\"font-size: 9px; margin-left: 1cm; display: none\"> <span class='title'></span></div> <div style=\"font-size: 9px; margin-left: auto; margin-right: 1cm; display: none\"> <span class='date'></span></div>",
  "redhat.telemetry.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false
}
